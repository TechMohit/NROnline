<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="grossary.cyron.com.grossary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="grossary.cyron.com.grossary.account.SigninActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="grossary.cyron.com.grossary.account.SignupActivity" />
        <activity android:name="grossary.cyron.com.grossary.profile.ProfileActivity" />
        <activity android:name="grossary.cyron.com.grossary.HomeActivity" />
        <activity android:name="grossary.cyron.com.grossary.category.CategoryActivity" />
    </application>

</manifest>